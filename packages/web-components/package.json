{
  "name": "components",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "wireit",
    "build": "wireit",
    "dev": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "tsc": {
      "command": "tsc",
      "files": ["src/**/*.ts"],
      "output": ["out-tsc/**/*.*"]
    },
    "docs": {
      "command": "storybook build -o docs"
    },
    "build": {
      "command": "vite",
      "dependencies": ["test", "tsc", "docs"],
      "files": ["out-tsc/**/*.*"],
      "output": ["dist/**/*.*"]
    },
    "dev": {
      "command": "storybook dev -p 6006",
      "files": [
        "stories/**/*.stories.ts",
        "src/**/*.*"
      ]
    },
    "test": {
      "command": "jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@floating-ui/dom": "^1.2.1",
    "@lit-labs/task": "^2.0.0",
    "@lit-labs/virtualizer": "^1.0.1",
    "lit": "^2.6.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@lit-labs/react": "^1.1.1",
    "@lit-labs/testing": "^0.2.0",
    "@storybook/addon-essentials": "^7.0.0-rc.0",
    "@storybook/addon-links": "^7.0.0-rc.0",
    "@storybook/blocks": "^7.0.0-alpha.8",
    "@storybook/web-components": "^7.0.0-rc.0",
    "@storybook/web-components-vite": "^7.0.0-alpha.35",
    "jest": "^29.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.17.2",
    "storybook": "^7.0.0-rc.0",
    "wireit": "^0.9.5"
  }
}
