{
  "name": "web-components",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsc": "wireit",
    "build": "wireit",
    "dev": "wireit",
    "test": "wireit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "wireit": {
    "tsc": {
      "command": "tsc",
      "files": [
        "src/**/*.ts"
      ],
      "output": [
        "out-tsc/**/*.*"
      ]
    },
    "docs": {
      "command": "storybook build -o docs"
    },
    "build": {
      "command": "vite build",
      "files": [
        "src/**/*.*"
      ],
      "output": [
        "dist/**/*.*"
      ]
    },
    "dev": {
      "command": "storybook dev -p 6006",
      "files": [
        "src/**/*.*",
        ".storybook/**/*.*"
      ]
    },
    "test": {
      "command": "jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@floating-ui/dom": "^1.2.1",
    "@lit-labs/task": "^2.1.0",
    "@lit-labs/virtualizer": "^2.0.0",
    "lit": "^2.7.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@lit-labs/testing": "^0.2.1",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/web-components": "^7.0.2",
    "@storybook/web-components-vite": "^7.0.2",
    "jest": "^29.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.2",
    "vite": "^4.2.1",
    "wireit": "^0.9.5"
  }
}
